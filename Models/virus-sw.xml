 <animation>
   <type>spin</type>
   <object-name>prop</object-name>
   <property>engines/engine[0]/rpm</property>
   <factor>-1</factor>
   <center>
     <x-m>0</x-m>
     <y-m>0</y-m>
     <z-m>0</z-m>
   </center>
   <axis>
     <x>1</x>
     <y>0</y>
     <z>0</z>
   </axis>
 </animation>

 <animation>
   <type>select</type>
   <object-name>prop</object-name>
   <condition>
     <less-than>
       <property>engines/engine[0]/rpm</property>
       <value>500</value>
     </less-than>
   </condition>
 </animation>

 <animation>
  <type>select</type>
  <object-name>prop_fast</object-name>
  <condition>
   <greater-than>
    <property>engines/engine[0]/rpm</property>
    <value>400</value>
   </greater-than>
  </condition>
 </animation>

 <animation>
   <type>blend</type>
   <object-name>prop_fast</object-name>
   <property>engines/engine[0]/rpm</property>
   <factor>0.0002</factor>
   <max>1.0</max>
   <min>0.0</min>
 </animation>

 <animation>
   <type>spin</type>
   <object-name>prop_fast</object-name>
   <property>engines/engine[0]/rpm</property>
   <factor>-0.001</factor>
   <center>
     <x-m>0</x-m>
     <y-m>0</y-m>
     <z-m>0</z-m>
   </center>
   <axis>
     <x>1</x>
     <y>0</y>
     <z>0</z>
   </axis>
 </animation>

<!-- gear compression animation  -->
 <animation>
   <type>translate</type>
   <object-name>w_shaft</object-name>
   <object-name>fairing_front</object-name>
   <object-name>wheel_front</object-name>
   <property>gear/gear[0]/compression-norm</property>
   <factor>0.30</factor>
   <axis>
     <x> 0.522 </x>
     <y> 0 </y>
     <z> 1.0 </z>
   </axis>
 </animation>

  <animation>
    <type>rotate</type>
    <object-name>strut_left</object-name>
    <object-name>fairing_left</object-name>
    <object-name>wheel_left</object-name>
    <property>gear/gear[1]/compression-norm</property>
    <interpolation>
      <entry><ind> 0 </ind><dep>  0 </dep></entry>
      <entry><ind> 1 </ind><dep> 24 </dep></entry>
    </interpolation>
    <center>
      <x-m>  0.274 </x-m>
      <y-m> -0.300 </y-m>
      <z-m> -0.460 </z-m>
    </center>
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>


  <animation>
    <type>rotate</type>
    <object-name>strut_right</object-name>
    <object-name>fairing_right</object-name>
    <object-name>wheel_right</object-name>
    <property>gear/gear[2]/compression-norm</property>
    <interpolation>
      <entry><ind> 0 </ind><dep>  0 </dep></entry>
      <entry><ind> 1 </ind><dep> 24 </dep></entry>
    </interpolation>
    <center>
      <x-m>  0.274 </x-m>
      <y-m>  0.300 </y-m>
      <z-m> -0.460 </z-m>
    </center>
    <axis>
      <x>  1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>
<!-- end of gear compression animation -->

</PropertyList>
