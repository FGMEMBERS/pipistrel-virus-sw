<!-- propeller animation -->
 <animation>
   <type>spin</type>
   <object-name>prop</object-name>
   <property>engines/engine[0]/rpm</property>
   <factor>-1</factor>
   <center>
     <x-m>0</x-m>
     <y-m>0</y-m>
     <z-m>0</z-m>
   </center>
   <axis>
     <x>1</x>
     <y>0</y>
     <z>0</z>
   </axis>
 </animation>

 <animation>
   <type>select</type>
   <object-name>prop</object-name>
   <condition>
     <less-than>
       <property>engines/engine[0]/rpm</property>
       <value>500</value>
     </less-than>
   </condition>
 </animation>

 <animation>
  <type>select</type>
  <object-name>prop_fast</object-name>
  <condition>
   <greater-than>
    <property>engines/engine[0]/rpm</property>
    <value>400</value>
   </greater-than>
  </condition>
 </animation>

 <animation>
   <type>blend</type>
   <object-name>prop_fast</object-name>
   <property>engines/engine[0]/rpm</property>
   <factor>0.0002</factor>
   <max>1.0</max>
   <min>0.0</min>
 </animation>

 <animation>
   <type>spin</type>
   <object-name>prop_fast</object-name>
   <property>engines/engine[0]/rpm</property>
   <factor>-0.001</factor>
   <center>
     <x-m>0</x-m>
     <y-m>0</y-m>
     <z-m>0</z-m>
   </center>
   <axis>
     <x>1</x>
     <y>0</y>
     <z>0</z>
   </axis>
 </animation>
<!-- end of propeller animation -->

<!-- aileron animation -->
 <animation>
  <object-name>aileron_left</object-name>
  <type>rotate</type>
  <property>surface-positions/left-aileron-pos-norm</property>
  <factor>25</factor>
  <center>
   <x-m> 0.73</x-m>
   <y-m>-2.40</y-m>
   <z-m> 0.50</z-m>
  </center>
  <axis>
   <x> 0   </x>
   <y> 1   </y>
   <z>-0.03</z>
  </axis>
 </animation>
 
  <animation>
  <object-name>aileron_right</object-name>
  <type>rotate</type>
  <property>surface-positions/right-aileron-pos-norm</property>
  <factor>25</factor>
  <center>
   <x-m> 0.73</x-m>
   <y-m> 2.40</y-m>
   <z-m> 0.50</z-m>
  </center>
  <axis>
   <x> 0   </x>
   <y>-1   </y>
   <z>-0.03</z>
  </axis>
 </animation>
<!-- end of aileron animation -->

<!-- rudder animation -->
 <animation>
     <type>rotate</type>
     <object-name>rudder</object-name>
     <property>surface-positions/rudder-pos-norm</property>
     <factor>19</factor>
     <center>
         <x-m>4.39</x-m>
         <y-m>0   </y-m>
         <z-m>0.17</z-m>
     </center>
     <axis>
         <x>0.25</x>
         <y>0  </y>
         <z>1  </z>
     </axis>
 </animation>
<!-- end of rudder animation -->

<!-- front wheel animation -->
 <animation>
     <type>rotate</type>
     <object-name>fairing_front</object-name>
     <object-name>wheel_front</object-name>
     <property>surface-positions/rudder-pos-norm</property>
     <factor>30</factor>
     <center>
         <x-m>-1.08</x-m>
         <y-m> 0.00</y-m>
         <z-m>-0.77</z-m>
     </center>
     <axis>
         <x> 0 </x>
         <y> 0 </y>
         <z>-1 </z>
     </axis>
 </animation>
<!-- end of front wheel animation -->

<!-- elevator animation -->
 <animation>
   <type>rotate</type>
   <object-name>elevator</object-name>
   <property>surface-positions/elevator-pos-norm</property>
   <factor>25</factor>
    <center>
      <x-m>  4.50 </x-m>
      <y-m>  0   </y-m>
      <z-m>  0.74 </z-m>
    </center>
    <axis>
      <x>  0 </x>
      <y>  1 </y>
      <z>  0 </z>
    </axis>
 </animation>
<!-- end of elevator animation -->

<!-- gear compression animation  -->
 <animation>
   <type>translate</type>
   <object-name>w_shaft</object-name>
   <object-name>fairing_front</object-name>
   <object-name>wheel_front</object-name>
   <property>gear/gear[0]/compression-norm</property>
   <factor>0.30</factor>
   <axis>
     <x> 0.522 </x>
     <y> 0 </y>
     <z> 1.0 </z>
   </axis>
 </animation>

  <animation>
    <type>rotate</type>
    <object-name>strut_left</object-name>
    <object-name>fairing_left</object-name>
    <object-name>wheel_left</object-name>
    <property>gear/gear[1]/compression-norm</property>
    <interpolation>
      <entry><ind> 0 </ind><dep>  0 </dep></entry>
      <entry><ind> 1 </ind><dep> 24 </dep></entry>
    </interpolation>
    <center>
      <x-m>  0.274 </x-m>
      <y-m> -0.300 </y-m>
      <z-m> -0.460 </z-m>
    </center>
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>


  <animation>
    <type>rotate</type>
    <object-name>strut_right</object-name>
    <object-name>fairing_right</object-name>
    <object-name>wheel_right</object-name>
    <property>gear/gear[2]/compression-norm</property>
    <interpolation>
      <entry><ind> 0 </ind><dep>  0 </dep></entry>
      <entry><ind> 1 </ind><dep> 24 </dep></entry>
    </interpolation>
    <center>
      <x-m>  0.274 </x-m>
      <y-m>  0.300 </y-m>
      <z-m> -0.460 </z-m>
    </center>
    <axis>
      <x>  1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>
<!-- end of gear compression animation -->

</PropertyList>
