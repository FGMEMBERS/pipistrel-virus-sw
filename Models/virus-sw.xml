<?xml version="1.0" ?>  
<PropertyList>  
	    
 
  <path>virus-sw.ac</path>


<effect>
    <inherits-from>Effects/model-transparent</inherits-from>
    <object-name>wind_shield</object-name>
 </effect>


	<offsets> 
		<heading-deg>-90</heading-deg>
		<z-m>0.27</z-m>
	</offsets>
	

  
	<animation>
		<condition>
			<not>
				<property>sim/rendering/rembrandt/enabled</property>
			</not>
		</condition>  
		
		<type>material</type> 
		<object-name>fuslage</object-name>
		<object-name>wind_shield</object-name>
		<object-name>nose</object-name>
		<object-name>prop</object-name>
		<object-name>back_wheel</object-name>
		<object-name>back_fairing</object-name>
		<object-name>back_strut</object-name>
		<object-name>front_wheel</object-name>
		<object-name>front_fairing</object-name>
		<object-name>front_strut</object-name>
		<object-name>front_w_shaft</object-name>
		
		<property-base>sim/model/livery/</property-base> 
		<texture-prop>texture</texture-prop>  
		<!--<texture>Liveries/PPA.png</texture>-->
	</animation>
	
	
 <animation>
   <type>select</type>
   <object-name>prop</object-name>
   <condition>
     <less-than>
       <property>engines/engine[0]/rpm</property>
       <value>500</value>
     </less-than>
   </condition>
 </animation>

