 <animation>
   <type>spin</type>
   <object-name>prop</object-name>
   <property>engines/engine[0]/rpm</property>
   <factor>-1</factor>
   <center>
     <x-m>0</x-m>
     <y-m>0</y-m>
     <z-m>0</z-m>
   </center>
   <axis>
     <x>0</x>
     <y>1</y>
     <z>0</z>
   </axis>
 </animation>

 <animation>
   <type>select</type>
   <object-name>prop</object-name>
   <condition>
     <less-than>
       <property>engines/engine[0]/rpm</property>
       <value>500</value>
     </less-than>
   </condition>
 </animation>

 <animation>
  <type>select</type>
  <object-name>prop_fast</object-name>
  <condition>
   <greater-than>
    <property>engines/engine[0]/rpm</property>
    <value>400</value>
   </greater-than>
  </condition>
 </animation>

 <animation>
   <type>blend</type>
   <object-name>prop_fast</object-name>
   <property>engines/engine[0]/rpm</property>
   <factor>0.0002</factor>
   <max>1.0</max>
   <min>0.0</min>
 </animation>

 <animation>
   <type>spin</type>
   <object-name>prop_fast</object-name>
   <property>engines/engine[0]/rpm</property>
   <factor>-0.001</factor>
   <center>
     <x-m>0</x-m>
     <y-m>0</y-m>
     <z-m>0</z-m>
   </center>
   <axis>
     <x>0</x>
     <y>1</y>
     <z>0</z>
   </axis>
 </animation>

<!--Gear Animation-->
  <animation>
    <type>rotate</type>
    <object-name>strut_left</object-name>
    <object-name>wheel_left</object-name>
    <object-name>fairing_left</object-name>
    <property>/gear/gear[1]/compression-norm</property>
   <interpolation>
     <entry>
       <ind>0</ind>
       <dep>5</dep>
     </entry>
     <entry>
       <ind>0.2</ind>
       <dep>-0</dep>
     </entry>
   </interpolation>
    <center>
      <x-m>0.30</x-m>
      <y-m>0.33</y-m>
      <z-m>-0.40</z-m>
    </center>
    <axis>
      <x>0.0</x>
      <y>1.0</y>
      <z>0.0</z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>strut_right</object-name>
    <object-name>wheel_right</object-name>
    <object-name>fairing_right</object-name>
    <property>/gear/gear[2]/compression-norm</property>
   <interpolation>
     <entry>
       <ind>0</ind>
       <dep>-5</dep>
     </entry>
     <entry>
       <ind>0.2</ind>
       <dep>0</dep>
     </entry>
   </interpolation>
    <center>
      <x-m>-0.30</x-m>
      <y-m>0.33</y-m>
      <z-m>-0.40</z-m>
    </center>
    <axis>
      <x>0.0</x>
      <y>1.0</y>
      <z>0.0</z>
    </axis>
  </animation>
 
</PropertyList>
